generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model user {
  id            String    @id @default(uuid())
  username      String
  password      String
  email         String
  emailVerified DateTime?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
